{
  "name": "tourze/wechat-open-platform-bundle",
  "description": "微信开放平台",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "ext-simplexml": "*",
    "doctrine/dbal": "^3.7.0 || ^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^2.20 || ^3.0",
    "doctrine/persistence": "^3.1 || ^4",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "tourze/easy-admin-attribute": "0.0.*",
    "tourze/doctrine-track-bundle": "0.0.*",
    "symfony/yaml": "^6.4 || ^7.1",
    "tourze/weui-bundle": "0.0.*"
  },
  "autoload": {
    "psr-4": {
      "WechatOpenPlatformBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WechatOpenPlatformBundle\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  }
}